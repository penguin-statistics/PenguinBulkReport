<BorderPane  xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PenguinBulkReport.BulkReportController">
   <center>
        <VBox prefHeight="376.0" prefWidth="640.0">
            <children>
                <Pane prefHeight="359.0" prefWidth="640.0">
                    <children>
                        <ListView fx:id="stage_list" layoutY="31.0" prefHeight="164.0" prefWidth="200.0" />
                        <Label layoutX="27.0" layoutY="5.0" text="Stage" />
                        <Button fx:id="add_stage_button" layoutX="100.0" layoutY="2.0" mnemonicParsing="false" text="Add " />
                        <GridPane fx:id="result_grid" layoutX="200.0" layoutY="29.0" prefHeight="320.0" prefWidth="439.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              </rowConstraints>
                        </GridPane>
                        <Label layoutX="378.0" layoutY="14.0" text="Results" />
                        <Label layoutX="41.0" layoutY="201.0" text="User Id" />
                        <TextField fx:id="user_id_field" layoutY="222.0" prefHeight="25.0" prefWidth="200.0" />
                        <Button fx:id="login_button" layoutX="100.0" layoutY="197.0" mnemonicParsing="false" onAction="#user_login" text="Login" />
                    </children>
                </Pane>
            </children>
        </VBox>
   </center>
</BorderPane>
